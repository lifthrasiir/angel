{
  "name": "angel",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-frontend": "npm run build --prefix frontend",
    "build-backend": "goimports -w . && go build -ldflags=\"-w\" .",
    "build": "npm run build-frontend && npm run build-backend",
    "test-backend": "goimports -w . && go test -count=3 -timeout=20s . ./src/editor ./src/filesystem ./src/internal/chat ./src/internal/env ./src/internal/llm",
    "test": "npm run test-backend",
    "pack": "upx --lzma -9 --force-overwrite -o angel.dist.exe angel.exe",
    "start": "node -e \"require('child_process').execSync(process.platform === 'win32' ? 'angel.exe' : '.\\angel', { stdio: 'inherit' })"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {}
}